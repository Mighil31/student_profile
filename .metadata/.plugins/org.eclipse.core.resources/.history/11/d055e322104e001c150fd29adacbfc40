package student_profile.java.model;

import java.util.List;

public class StudentDetails {
	
	public class Certifications {
		public int userID;
		public String title;
		public String description;
		public String period;
		public String imageURL;
		
		public Certifications(int userID, String title, String description, String period, String imageURL) {
			super();
			this.userID = userID;
			this.title = title;
			this.description = description;
			this.period = period;
			this.imageURL = imageURL;
		}
		
	}
	
	public class Skills {
		public int userID;
		public String skillTitle;
		public Skills(int userID, String skillTitle) {
			super();
			this.userID = userID;
			this.skillTitle = skillTitle;
		}
	}
	
	public class Education {
		public int userID;
		public String education;
		public String qualification;
		public String period;
		public String imageURL;
		
		public Education(int userID, String education, String qualification, String period, String imageURL) {
			super();
			this.userID = userID;
			this.education = education;
			this.qualification = qualification;
			this.period = period;
			this.imageURL = imageURL;
		}
		
	}
	
	public Student student;
	public List<Certifications> certifications;
	public List<Skills> skills;
	public List<Education> educations;
	
	public StudentDetails(Student student) {
		super();
		this.student = student;
	} 

	
}
